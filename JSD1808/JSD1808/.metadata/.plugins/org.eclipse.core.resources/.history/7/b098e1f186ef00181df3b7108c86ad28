import org.junit.Test;

/**
 * 对DBUtils_Test 的测试
 * @author OldFarHigh
 *
 */
public class Test01 {
	@Test
	public void findAll() {
		Connection conn = null;
		PreparedStatement stat = null;
		ResultSet rs = null;
		try {
			conn = DBUtils_Test.getConn();
			String sql = "select * from emp";
			stat = conn.prepareStatement(sql);
			

		} catch (Exception e) {
			e.printStackTrace();
		} finally {
			DBUtils_Test.close(conn, stat, rs);
		}
	}
}
