package cn.tedu.spring;
/**
 * Model.Service
 * @author OldFarHigh
 *
 */
public class UserService {

	UserDao userdao = new UserDao();
	//模拟：注册
	public void reg() {
		System.out.println("UserService.reg()");
		
		dao.findUserByUsername();
	}
	//模拟：登录
	public void login() {
		System.out.println("UserService.login()");
		
		UserDao dao = new UserDao();
		dao.findUserByUsername();
	}
}
