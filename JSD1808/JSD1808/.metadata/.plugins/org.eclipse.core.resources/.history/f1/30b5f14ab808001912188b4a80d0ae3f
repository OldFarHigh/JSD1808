<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!--namespace: 映射的接口的全名 -->
<mapper namespace="cn.tedu.ajax.mapper.UserMapper">
	<!-- 根据执行的SQL种类选择节点 -->
	<!-- id：抽象方法的名称 parameterType：抽象方法的参数类型 -->
	
	<select id="findByUsername" resultMap="cn.tedu.ajax.entity.User">
		SELECT id,password FROM t_user WHRER username= #{username}
	</select>
	<insert id="insert">
		INSERT INTO t_user(
			username,password,age,
			phone,email,is_delete,
			department
		)VALUES(
			#{username},#{password},#{age},
			#{phone},#{email},#{isDelete},
			#{department}
		)
	</insert>

</mapper>

